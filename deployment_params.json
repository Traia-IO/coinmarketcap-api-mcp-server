{
  "github_url": "https://github.com/Traia-IO/coinmarketcap-api-mcp-server",
  "mcp_server": {
    "name": "coinmarketcap-api-mcp",
    "description": "Coinmarketcap cryptocurrency market data api - comprehensive price, market cap, volume, and market metrics",
    "server_type": "streamable-http",
    "requires_api_key": true,
    "api_key_header": "Authorization",
    "api_keys": [
      "COINMARKETCAP_API_API_KEY"
    ],
    "capabilities": [
      "get_cryptocurrency_listings_latest",
      "get_cryptocurrency_quotes_latest",
      "get_cryptocurrency_map",
      "get_cryptocurrency_info",
      "get_cryptocurrency_categories",
      "get_cryptocurrency_category",
      "get_global_metrics_latest",
      "get_fiat_map",
      "get_cryptocurrency_ohlcv_latest",
      "get_cryptocurrency_trending_latest"
    ],
    "payment_protocol": "402",
    "dual_mode": true,
    "dual_mode_description": "Supports both authenticated (free with API key) and payment (402 protocol) modes"
  },
  "deployment_method": "cloud_run",
  "gcp_project_id": "traia-mcp-servers",
  "gcp_region": "us-central1",
  "tags": [
    "coinmarketcap api",
    "api",
    "mcp"
  ],
  "ref": "main"
}