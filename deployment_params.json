{
  "github_url": "https://github.com/Traia-IO/coinmarketcap-api-mcp-server",
  "mcp_server": {
    "name": "coinmarketcap-api-mcp",
    "description": "Coinmarketcap cryptocurrency data api",
    "server_type": "streamable-http",
    "requires_api_key": true,
    "api_key_header": "Authorization",
    "api_keys": [
      "COINMARKETCAP_API_KEY"
    ],
    "capabilities": [
      "airdrops",
      "categories",
      "category",
      "listings_historical",
      "listings_latest",
      "listings_new",
      "coinmarketcap_id_map",
      "trending_gainers_losers",
      "trending_latest",
      "trending_most_visited",
      "exchange_assets",
      "metadata",
      "coinmarketcap_id_map_wpht",
      "quotes_historical",
      "coinmarketcap_id_map_1b83",
      "quotes_historical_m0v0",
      "quotes_latest_qen5",
      "key_info",
      "postman_conversion_v1",
      "metadata_v2",
      "ohlcv_historical_v2",
      "ohlcv_latest_v2",
      "price_performance_stats_v2",
      "quotes_historical_v2",
      "quotes_latest_v2",
      "price_conversion_v2",
      "quotes_historical_v3",
      "cmc_crypto_fear_and_greed_historical",
      "cmc_crypto_fear_and_greed_latest"
    ],
    "payment_protocol": "402",
    "dual_mode": true,
    "dual_mode_description": "Supports both authenticated (free with API key) and payment (402 protocol) modes"
  },
  "deployment_method": "cloud_run",
  "gcp_project_id": "traia-mcp-servers",
  "gcp_region": "us-central1",
  "tags": [
    "coinmarketcap api",
    "api",
    "mcp"
  ],
  "ref": "main"
}